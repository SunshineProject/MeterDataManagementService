package it.sinergis.sunshine.sos40.pojo;

import java.io.Serializable;
import java.util.Date;

import javax.persistence.Column;
import javax.persistence.Entity;
import javax.persistence.Id;
import javax.persistence.Table;

@Entity
@Table(name = "observation", schema = "public")
public class Observation implements Serializable {
	@Id
	@Column(name = "observationid")
	private int observationid;
	@Column(name = "seriesid")
	private int seriesid;
	@Column(name = "phenomenontimestart")
	private Date phenomenontimestart;
	@Column(name = "phenomenontimeend")
	private Date phenomenontimeend;
	@Column(name = "resulttime")
	private Date resulttime;
	@Column(name = "identifier")
	private String identifier;
	@Column(name = "codespaceid")
	private Integer codespaceid;
	@Column(name = "description")
	private String description;
	@Column(name = "deleted")
	private boolean deleted;
	@Column(name = "validtimestart")
	private Date validtimestart;
	@Column(name = "validtimeend")
	private Date validtimeend;
	@Column(name = "unitid")
	private Integer unitid;
	
	public int getObservationid() {
		return observationid;
	}
	
	public void setObservationid(int observationid) {
		this.observationid = observationid;
	}
	
	public Date getPhenomenontimestart() {
		return phenomenontimestart;
	}
	
	public void setPhenomenontimestart(Date phenomenontimestart) {
		this.phenomenontimestart = phenomenontimestart;
	}
	
	public Date getPhenomenontimeend() {
		return phenomenontimeend;
	}
	
	public void setPhenomenontimeend(Date phenomenontimeend) {
		this.phenomenontimeend = phenomenontimeend;
	}
	
	public Date getResulttime() {
		return resulttime;
	}
	
	public void setResulttime(Date resulttime) {
		this.resulttime = resulttime;
	}
	
	public String getIdentifier() {
		return identifier;
	}
	
	public void setIdentifier(String identifier) {
		this.identifier = identifier;
	}
	
	public Integer getCodespaceid() {
		return codespaceid;
	}
	
	public void setCodespaceid(Integer codespaceid) {
		this.codespaceid = codespaceid;
	}
	
	public String getDescription() {
		return description;
	}
	
	public void setDescription(String description) {
		this.description = description;
	}
	
	public boolean isDeleted() {
		return deleted;
	}
	
	public void setDeleted(boolean deleted) {
		this.deleted = deleted;
	}
	
	public Date getValidtimestart() {
		return validtimestart;
	}
	
	public void setValidtimestart(Date validtimestart) {
		this.validtimestart = validtimestart;
	}
	
	public Date getValidtimeend() {
		return validtimeend;
	}
	
	public void setValidtimeend(Date validtimeend) {
		this.validtimeend = validtimeend;
	}
	
	public Integer getUnitid() {
		return unitid;
	}
	
	public void setUnitid(Integer unitid) {
		this.unitid = unitid;
	}
	
	public int getSeriesid() {
		return seriesid;
	}
	
	public void setSeriesid(int seriesid) {
		this.seriesid = seriesid;
	}
	
}
